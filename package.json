{
  "name": "premiacaovpio",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "dev:api": "pnpm --filter @sistema-premiacao/api dev",
    "dev:web": "pnpm --filter @sistema-premiacao/web dev"
  },
  "devDependencies": {
    "dotenv": "^16.5.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.2",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
